{
  "vim.leader": "[",
  "vim.easymotion": true,
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "g"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "<leader>",
        "j"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "r"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "editor.action.refactor"
      ]
    },
    {
      "before": [
        "<leader>",
        "c"
      ],
      "commands": [
        "editor.action.goToReferences"
      ]
    },
    {
      "before": [
        ";",
        "t"
      ],
      "commands": [
        "testing.runAll"
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "commands": [
        "editor.debug.action.toggleBreakpoint"
      ]
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "commands": [
        "workbench.action.debug.start"
      ]
    },
    {
      "before": [
        "<leader>",
        "o"
      ],
      "commands": [
        "workbench.action.debug.stepOver"
      ]
    },
    {
      "before": [
        "enter"
      ],
      "commands": [
        "editor.action.format"
      ]
    },
    {
      // leader -s for "split
      "before": [
        "<leader>",
        "s"
      ],
      "commands": [
        "workbench.action.splitEditorRight"
      ]
    },
    {
      "before": [
        "<leader>",
        "t"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "]",
        "t"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
  ],
  "vim.handleKeys": {
    "f": false,
    "/": false,
    "<C-t>": false,
    "<C-s>": false,
    "<C-z>": false,
    "<C-PgUp>": false,
    "<C-PgDown>": false,
  },
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "editor.action.refactor"
      ]
    }
  ],
  "vim.useSystemClipboard": true,
  // "workbench.colorCustomizations": {
  //     "editorGroupHeader.tabsBackground": "#1A1A1A",
  //     "editor.background": "#1A1A1A",
  //     "tab.activeBackground": "#2e19f018",
  //     "tab.activeForeground": "#1c83fa",
  //     "titleBar.activeBackground": "#1A1A1A",
  //     "titleBar.activeForeground": "#1c83fa",
  //     "statusBar.background": "#1A1A1A",
  //     "statusBar.focusBorder": "#1A1A1A",
  //     "activityBar.background": "#1A1A1A",
  //     "activityBar.foreground": "#1c83fa",
  //     "editorCursor.foreground": "#ffffff",
  //     "sideBar.background": "#1A1A1A",
  //     "sideBar.foreground": "#1c83fa",
  //   },
  "editor.inlineSuggest.enabled": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "#1c83fa",
  "python.experiments.enabled": false,
  "editor.accessibilitySupport": "off",
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "c": false,
    "cpp": false,
    "bat": true,
    "rust": false
  },
  "window.menuBarVisibility": "compact",
  "editor.formatOnSave": true,
  "html.autoClosingTags": false,
  "debug.console.wordWrap": false,
  "css.lint.unknownAtRules": "ignore",
  "git.ignoreRebaseWarning": true,
  "git.confirmSync": false,
  "github.gitAuthentication": false,
  "workbench.layoutControl.enabled": false,
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "javascript.autoClosingTags": false,
  "typescript.autoClosingTags": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "workbench.colorTheme": "GitHub Dark",
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.highlightRelated.breakPoints.enable": false,
  "debug.allowBreakpointsEverywhere": true,
  "hexeditor.columnWidth": 4,
  "hexeditor.showDecodedText": false,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
}